{# @pebvariable name="user" type="org.example.model.User" #}

{% extends '../layout.peb' %}

{% block content %}
<main>
  {% if user is not null %}
    <dl>
      <dt>User ID</dt>
      <dd>{{ user.id }}</dd>
      <dt>Name</dt>
      <dd>{{ user.name }}</dd>
      <dt>Email</dt>
      <dd>{{ user.email }}</dd>
    </dl>
  {% endif %}
</main>
{% endblock %}
